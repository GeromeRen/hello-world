pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        git 'https://github.com/GeromeRen/hello-world.git'
      }
    }

    stage('Build') {
      steps {
        sh '''
        export MAVEN_HOME=/opt/maven
        export PATH=$PATH:$MAVEN_HOME/bin
        mvn --version
        mvn clean install
        ‘’‘
      }
    }
  }

}
